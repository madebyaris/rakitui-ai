{
  "name": "rakitui-ai",
  "version": "1.0.6",
  "description": "An intelligent UI component design selection tool built on the Model Context Protocol (MCP). Allows AI assistants to present multiple UI designs for user selection.",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "rakitui-ai": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist/tools/* && tsc && mcp-build",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "test": "node test-improved.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.4",
    "@types/ws": "^8.18.1",
    "mcp-framework": "^0.2.15",
    "open": "^10.1.2",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.19.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ui",
    "design",
    "selection",
    "ai",
    "claude",
    "component",
    "tool",
    "frontend"
  ],
  "author": "Aris Setiawan <arissetia.m@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/rakitui-ai.git"
  },
  "homepage": "https://github.com/your-username/rakitui-ai#readme",
  "bugs": {
    "url": "https://github.com/your-username/rakitui-ai/issues"
  }
}
